<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="537.0" spacing="20.0" styleClass="vbox" stylesheets="@../../../others/css.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.quancf.PrimaryController">
   <children>
      <Button fx:id="logout" mnemonicParsing="false" text="Đăng xuất">
         <VBox.margin>
            <Insets bottom="50.0" left="400.0" />
         </VBox.margin>
      </Button>
      <Label fx:id="session" />
      <Button fx:id="clickMe" mnemonicParsing="false" onAction="#showDangNhap" text="Đăng nhập" />
      <PasswordField fx:id="password" promptText="Enter a password if you have" />
      <Button fx:id="login" mnemonicParsing="false" text="Truy cập" />
      <Button fx:id="datBan" onAction="#switchToSecondary" text="Đặt bàn" />
      <Button fx:id="datMon" mnemonicParsing="false" onAction="#switchToDatMon" text="Menu" />
      <Button fx:id="inHoaDon" mnemonicParsing="false" onAction="#switchToInHoaDon" text="In hóa đơn" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
